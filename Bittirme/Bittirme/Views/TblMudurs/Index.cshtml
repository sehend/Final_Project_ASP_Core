@model IEnumerable<Core.Models.TblMudur>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<a href="@Url.Action("Index","Ekrankartis")" class="btn btn-primary">Ekrankartis</a>
<a href="@Url.Action("Index","mice")" class="btn btn-primary">Mouse</a>
<a href="@Url.Action("Index","rams")" class="btn btn-primary">rams</a>
<a href="@Url.Action("Index","ssds")" class="btn btn-primary">ssds</a>
<a href="@Url.Action("Index","TblMusteris")" class="btn btn-primary">MüşteriLer</a>
<a href="@Url.Action("Index","TblPersonels")" class="btn btn-primary">Personel</a>
<a href="@Url.Action("Index","TblSatis")" class="btn btn-primary">Satıs</a>


<a href="@Url.Action("PersonelExcel","Excel")" class="btn btn-primary">Personel Excel</a>
<a href="@Url.Action("MusterisExcel","Excel")" class="btn btn-primary">Müsteri Excel</a>
<a href="@Url.Action("RamExcel","Excel")" class="btn btn-primary">RamExcel</a>
<a href="@Url.Action("EkranKartıExcel","Excel")" class="btn btn-primary">EkranKartıExcel</a>
<a href="@Url.Action("mouseExcel","Excel")" class="btn btn-primary">mouseExcel</a>
<a href="@Url.Action("ssdExcel","Excel")" class="btn btn-primary">ssdExcel</a>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.imageName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Soyad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MüdürMaaş)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Urunıd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adminıd)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <img src="~/image/Mudur/@item.imageName" asp-append-version="true" width="50" height="50" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ad)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Soyad)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Adress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MüdürMaaş)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Urunıd)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Adminıd)
                </td>
                <td>
                   
                    <a asp-action="Details" asp-route-id="@item.Müdürıd">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Müdürıd">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
